  "main": "dist/cjs/index.js",
  "module": "dist/es6/index.js",
  "types": "dist/cjs/index.d.ts",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/waynevanson/fp-ts-webdriver.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && npm-run-all --parallel build:*",
